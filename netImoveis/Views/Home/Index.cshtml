@{
    ViewData["Title"] = "Home Page";
}

<div class="text-center">
    <h1 class="display-4">Net Imóveis</h1>
    <p>Learn about <a href="ExibicaoImoveis">Conheça nossos imóveis</a></p>
</div>

@model netImoveis.Models.ExibicaoImoveisViewModel


<div>
    @if (Model.ListaImoveis != null && Model.ListaImoveis.Count() > 0)
    {
    <table>
        @foreach (var prod in Model.ListaImoveis)
        {
            <br>
            <tr>
                <td>@Html.Raw(prod)</td>
            </tr>
        }

    </table>
    }
</div>